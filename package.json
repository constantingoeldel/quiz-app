{
  "name": "quiz-app",
  "version": "1.0.0",
  "description": "A web-app to test yourself",
  "main": "index.html",
  "scripts": {
    "start": "npm run sass && npm-run-all --parallel serve sass:watch",
    "serve": "serve .",
    "sass": "node-sass --output-style compressed  scss/styles.scss styles.css",
    "sass:watch": "npm run sass -- -w"
  },
