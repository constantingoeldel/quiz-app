  "bugs": {
    "url": "https://github.com/constantingoeldel/quiz-app/issues"
  },
  "homepage": "https://github.com/constantingoeldel/quiz-app#readme",
  "dependencies": {
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "serve": "^11.3.2"
  }
}
